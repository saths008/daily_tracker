<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextInputControl?>

   <VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tracker.SpreadSheetController">
      <children>
      <Button text="Home" onAction="#goHome"/>
      <Label text="FilePath:" />
         <TextField fx:id="filePathTextField" />
         <Button text="Select File" onAction="#selectFile" />

      <Label text="Column Titles (Date and Notes are included by default):" />
      <TextField fx:id="headersTextField" />

      <Button text="Submit" onAction="#initialiseSpreadSheet"/>
      <Label fx:id="initialiseSpreadSheetMessageLabel" />
      </children>
      <padding>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
      </padding>
   </VBox>
